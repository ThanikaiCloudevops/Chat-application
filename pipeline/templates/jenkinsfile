@Library ("my_shared_library") _ 
pipeline{
    agent any

    environment {
        GIT_URL = "https://SciflareIT@bitbucket.org/dpl11-backend/goo-plus.git"
        GIT_BRANCH = "seran_dev"
        IMAGE_NAME = 'alpine'
        IMAGE_TAG = 'latest'
        // DOCKERFILE_PATH
    }
    parameters {
        string(name: 'REPO_URL', defaultValue: 'https://SciflareIT@bitbucket.org/dpl11-backend/goo-plus.git', description: 'Repository URL')
        string(name: 'BRANCH', defaultValue: 'seran_dev', description: 'Branch to checkout')
    }

    stages{
        stage("Checkout-Code"){
            steps{
                script{
                    def checkoutCode = load checkoutCode
                    checkoutCode.call(params.REPO_URL,params.BRANCH)
                }
            }
        }
        // stage("Build Node"){
        //     steps{
        //         script{
        //             build()
        //         }
        //     }
        // }
    }
}

