@Library("my_shared_library") _

pipeline {
    agent any 

    environment{
        GITHUB_CREDS = credentails("BitbucketRepo")
    }

    parameters {
        string(name:"project",defaultValue: "gooplus-backend",description:"Project From Gooplus-Backend[Api,Cron]" )
    }
    stages{
        stage("Checkout"){
            steps{
                script{
                    checkout(params.project,GITHUB_CREDS)
                }
            }
        }
        // stage("Build"){
        //     steps{
        //         script{
        //             build()
        //         }
        //     }
        // }
        stage("Deploy"){
            steps{
                script{
                    echo "Tested Success"
                }
            }
        }
    }
}
