@Library ("my_shared_library") _ 
pipeline{
    agent any

    environment {
        IMAGE_NAME = 'alpine'
        IMAGE_TAG = 'latest'
        // DOCKERFILE_PATH
    }
    
    stages{
        stage("Checkout-Code"){
            steps{
                script{
                    checkout()
                }
            }
        }
        stage("Build Node"){
            steps{
                script{
                    build()
                }
            }
        }
    }
}

