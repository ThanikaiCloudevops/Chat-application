@Library("my_shared_library") _

pipeline {
    agent any 

    parameters {
        string(name:"project",defaultValue: "gooplus",description:"Project From Gooplus-Backend[Api,Cron]" )
    }
    stages{
        stage("Checkout"){
            steps{
                script{
                    checkout(project)
                }
            }
        }
        stage("Build"){
            steps{
                script{
                    build()
                }
            }
        }
        stage("Deploy"){
            steps{
                script{
                    echo "Tested Success"
                }
            }
        }
    }
}
