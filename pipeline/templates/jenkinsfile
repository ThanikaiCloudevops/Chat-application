@Library("my_shared_library") _

pipeline {
    agent any 

    environment {
        git_url = 'https://SciflareIT@bitbucket.org/dpl11-backend/goo-plus.git'
        git_branch = 'seran_dev'
    }
    stages{
        stage("Checkout"){
            steps{
                script{
                    git url: git_url , branch: git_branch
                }
            }
        }
        stage("Build"){
            steps{
                script{
                    build()
                }
            }
        }
        stage("Deploy"){
            steps{
                script{
                    echo "Tested Success"
                }
            }
        }
    }
}