@Library("my_shared_library") _

pipeline {
    agent any 

    environment {
        git_url = 'https://github.com/thanikai-devops/Devops_ansible.git'
        git_branch = 'feature-jenkins'
    }
    stages{
        stage("Checkout"){
            steps{
                script{
                    git url: git_url , branch: git_branch
                }
            }
        }
        stage("Build"){
            steps{
                script{
                    echo "Build Success"
                }
            }
        }
        stage("Deploy"){
            steps{
                script{
                    echo "Tested Success"
                }
            }
        }
    }
}