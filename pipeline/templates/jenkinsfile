@Library("my_shared_library") _

pipeline {
    agent any 

    parameters {
        string(name:"project",defaultValue: "gooplus-backend",description:"Project From Gooplus-Backend[Api,Cron]" )
    }
    stages{
        stage("Checkout"){
            steps{
                script{
                    def config = gitconfig(params.project)
                    echo "Git URL: ${config.git_url}"
                    echo "Git Branch: ${config.git_branch}"
                    checkout([
                            $class: 'GitSCM',
                            branches: [[name: "*/${config.git_branch}"]],
                            userRemoteConfigs: [[url: config.git_url, credentialsId: config.git_credentials]]
                        ])
                }
            }
        }
        // stage("Build"){
        //     steps{
        //         script{
        //             build()
        //         }
        //     }
        // }
        stage("Deploy"){
            steps{
                script{
                    echo "Tested Success"
                }
            }
        }
    }
}
