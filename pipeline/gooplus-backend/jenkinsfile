@Library ("my_shared_library") _ 
pipeline{
    agent any 

    parameter{
        string(name:"Branch",defaultValue:"seran_dev",description:"Checkout the branch")
        string(name:"Repo",defaultValue:"https://SciflareIT@bitbucket.org/dpl11-backend/goo-plus.git",description:"Checkout the url")
    }

    stages{
        stage("Checkout-Code"){
            steps{
                script{
                    checkoutgoo(
                        branch:params.Branch,
                        repoUrl:params.Repo)
                }
            }
        }
    }
}

